{
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ],
    "redirects": [
      {
        "source": "/:path*",
        "has": [
          {
            "type": "header",
            "key": "x-forwarded-proto",
            "value": "http"
          }
        ],
        "destination": "https://distancesinbulk.com/:path*",
        "permanent": true
      },
      {
        "source": "/:path*",
        "has": [
          {
            "type": "host",
            "value": "www.distancesinbulk.com"
          }
        ],
        "destination": "https://distancesinbulk.com/:path*",
        "permanent": true
      }
    ]
  }