{
    "redirects": [
      {
        "source": "/((?!.*\\.).*)",
        "has": [
          {
            "type": "header",
            "key": "x-forwarded-proto",
            "value": "http"
          }
        ],
        "destination": "https://distancesinbulk.com/:splat",
        "permanent": true
      },
      {
        "source": "/((?!.*\\.).*)",
        "has": [
          {
            "type": "host",
            "value": "www.distancesinbulk.com"
          }
        ],
        "destination": "https://distancesinbulk.com/:splat",
        "permanent": true
      }
    ]
  }