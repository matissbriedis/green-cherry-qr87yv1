{
    "redirects": [
      {
        "source": "/:path*",
        "has": [{ "type": "host", "value": "www.distancesinbulk.com" }],
        "destination": "https://distancesinbulk.com/:path*",
        "permanent": true
      },
      {
        "source": "/",
        "has": [{ "type": "header", "key": "x-forwarded-proto", "value": "http" }],
        "destination": "https://distancesinbulk.com/",
        "permanent": true
      }
    ]
  }